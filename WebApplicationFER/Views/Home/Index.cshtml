@model IEnumerable<WebApplicationFER.DataBase.User>

<h2>Список пользователей</h2>
<p><a asp-action="Create">Добавить пользователя</a></p>
<table class="table">
    <tr><th>Id</th><th>Имя</th><th>Фамилия</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Surname</td>
        </tr>
    }
</table>